{
  "name": "@recordset/adonis6-bullmq",
  "description": "BullMQ provider for AdonisJS 6",
  "version": "1.0.5",
  "engines": {
    "node": ">=20.6.0"
  },
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "build/commands",
    "build/providers",
    "build/services",
    "build/src",
    "build/stubs",
    "build/index.js",
    "build/index.d.ts",
    "build/configure.js",
    "build/configure.d.ts"
  ],
  "exports": {
    ".": "./build/index.js",
    "./commands": "./build/commands/main.js",
    "./commands/*": "./build/commands/*.js",
    "./types": "./build/src/types.js",
    "./job": "./build/src/job/base_job.js",
    "./services/*": "./build/services/*.js",
    "./bullmq_provider": "./build/providers/bullmq_provider.js",
    "./bullmq_manager": "./build/src/bullmq_manager.js"
  },
  "scripts": {
    "index:commands": "adonis-kit index build/commands",
    "build": "npm run clean && npm run build-only && npm run copyfiles && npm run index:commands",
    "build-only": "tsc",
    "clean": "del-cli build",
    "copyfiles": "copyfiles \"stubs/**/**/*.stub\" build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "node --test ./tests/**/*.spec.ts || japa ./tests/**/*.spec.ts",
    "release": "npx release-it",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "adonisjs",
    "adonisjs 6",
    "bullmq",
    "queue",
    "jobs",
    "redis"
  ],
  "author": "RecordSet",
  "license": "MIT",
  "homepage": "https://github.com/recordset/adonis6-bullmq#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/recordset/adonis6-bullmq.git"
  },
  "bugs": {
    "url": "https://github.com/recordset/adonis6-bullmq/issues"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/core": "^6.18.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^3.0.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.6.3",
    "@types/node": "^20.14.5",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "np": "^10.0.6",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.18.0",
    "bullmq": "^5.0.4"
  },
  "dependencies": {
    "bullmq": "^5.0.4"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@adonisjs/prettier-config"
}
